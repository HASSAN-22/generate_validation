{
    "name": "hasan-22/generate_validation",
    "description": "A Laravel package to automatically generate validation rules from models.",
    "keywords": [
        "laravel",
        "validation",
        "rules",
        "generator"
    ],
    "homepage": "https://github.com/HASSAN-22/generate_validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Hasan",
            "email": "armiaevil@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "GenerateValidation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GenerateValidation\\ValidationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}